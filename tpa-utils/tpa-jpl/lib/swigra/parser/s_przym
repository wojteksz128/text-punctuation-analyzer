chętny	cp(żeby)
chętny	prepnp(do,gen)
głodny	np(gen)
gotowy	infp(_)
należny	np(dat)
niezbędny	np(dat)+cp(żeby)
oburzony	cp(że)
pełny	np(gen)
pewien	cp(że)
pewien	np(gen)
pewny	cp(że)
pewny	np(gen)
potrzebny	np(dat)+cp(żeby)
przeciwny	np(dat)
przeciwny	ncp(dat,żeby)
przekonany	cp(że)
szczęśliwy	cp(że)
wierny	np(dat)
wierny	ncp(dat,że)
wściekły	cp(że)
zaangażowany	prepnp(w,acc)
zadowolony	cp(że)
zadowolony	prepncp(z,gen,że)
zadowolony	prepnp(z,gen)
zgodny	cp(że)
żądny	np(gen)
